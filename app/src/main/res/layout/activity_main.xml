<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tech.semo.fuel.MainActivity">

    <TextView
        android:id="@+id/sgtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="13dp"
        android:layout_marginTop="39dp"
        android:text="S.G" />

    <EditText
        android:id="@+id/sgvalue"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/sgtext"
        android:layout_toEndOf="@+id/tracktext"
        android:ems="10"
        android:hint="0"
        android:imeOptions="actionNext"
        android:inputType="numberDecimal" />

    <TextView
        android:id="@+id/b4text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/sgtext"
        android:layout_below="@+id/sgtext"
        android:layout_marginTop="27dp"
        android:text="B4" />

    <TextView
        android:id="@+id/aftext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/b4text"
        android:layout_below="@+id/b4text"
        android:layout_marginTop="27dp"
        android:text="AF" />

    <TextView
        android:id="@+id/deltatextkg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/aftext"
        android:layout_below="@+id/aftext"
        android:layout_marginTop="27dp"
        android:text="DIFF IN KG =" />


    <EditText
        android:id="@+id/b4value"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/ctankb4value"
        android:layout_alignBottom="@+id/ctankb4value"
        android:layout_alignParentEnd="true"
        android:ems="10"
        android:hint="Tot"
        android:imeOptions="actionNext"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/afvalue"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/ctankafvalue"
        android:layout_alignBottom="@+id/ctankafvalue"
        android:layout_alignStart="@+id/b4value"
        android:ems="10"
        android:hint="Tot"
        android:imeOptions="actionNext"
        android:inputType="numberDecimal" />

    <TextView
        android:id="@+id/deltavaluekg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/deltatextkg"
        android:layout_centerHorizontal="true"
        android:ems="10"
        android:imeOptions="actionNext" />

    <TextView
        android:id="@+id/deltatextlt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/deltatextkg"
        android:layout_below="@+id/deltatextkg"
        android:layout_marginTop="37dp"
        android:text="DIFF IN LT =" />

    <TextView
        android:id="@+id/deltavaluelt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/deltatextlt"
        android:layout_centerHorizontal="true"
        android:ems="10" />

    <TextView
        android:id="@+id/tracktext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/deltatextlt"
        android:layout_below="@+id/deltatextlt"
        android:layout_marginTop="20dp"
        android:text="TRK" />

    <EditText
        android:id="@+id/trk1value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/deltavaluelt"
        android:layout_alignStart="@+id/deltavaluelt"
        android:layout_alignTop="@+id/tracktext"
        android:autofillHints="0"
        android:ems="10"
        android:hint="0"
        android:imeOptions="actionNext"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/trk2value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/deltavaluelt"
        android:layout_alignStart="@+id/trk1value"
        android:layout_below="@+id/trk1value"
        android:layout_marginTop="10dp"
        android:autofillHints="0"
        android:ems="10"
        android:hint="0"
        android:imeOptions="actionNext"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/trk3value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/trk2value"
        android:layout_alignStart="@+id/trk2value"
        android:layout_below="@+id/trk2value"
        android:layout_marginTop="12dp"
        android:autofillHints="0"
        android:ems="10"
        android:hint="0"
        android:imeOptions="actionDone"
        android:inputType="numberDecimal" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/trk2value"
        android:layout_toEndOf="@+id/aftext"
        android:text="TRK1" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/trk3value"
        android:layout_alignStart="@+id/textView11"
        android:text="TRK2" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/trk3value"
        android:layout_alignStart="@+id/textView12"
        android:text="TRK3" />

    <TextView
        android:id="@+id/difftext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/deltatextkg"
        android:layout_below="@+id/textView17"
        android:layout_marginTop="26dp"
        android:text="DELTA %  =" />

    <TextView
        android:id="@+id/deltapercentvalue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/difftext"
        android:layout_centerHorizontal="true"
        android:ems="10" />

    <Button
        android:id="@+id/btncalc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/clrbtn"
        android:layout_below="@+id/trk1value"
        android:onClick="calc"
        android:text="CALC" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView13"
        android:layout_below="@+id/textView13"
        android:layout_marginTop="23dp"
        android:text="SUM =" />

    <TextView
        android:id="@+id/trksumvalue"
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:layout_alignBaseline="@+id/textView17"
        android:layout_alignBottom="@+id/difftext"
        android:layout_alignStart="@+id/trk3value" />

    <Button
        android:id="@+id/clrbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/trk1value"
        android:layout_alignStart="@+id/afvalue"
        android:onClick="clr"
        android:text="CLEAR" />

    <EditText
        android:id="@+id/ltankb4value"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/b4text"
        android:layout_alignStart="@+id/sgvalue"
        android:ems="10"
        android:hint="L"
        android:imeOptions="actionNext"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/rtankb4value"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView2"
        android:layout_alignStart="@+id/rtankafvalue"
        android:ems="10"
        android:hint="R"
        android:imeOptions="actionNext"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/ctankb4value"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView5"
        android:layout_toEndOf="@+id/textView5"
        android:ems="10"
        android:hint="C"
        android:imeOptions="actionNext"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/ltankafvalue"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/deltatextkg"
        android:layout_alignStart="@+id/ltankb4value"
        android:ems="10"
        android:hint="L"
        android:imeOptions="actionNext"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/rtankafvalue"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignEnd="@+id/trksumvalue"
        android:ems="10"
        android:hint="R"
        android:imeOptions="actionNext"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/ctankafvalue"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/rtankafvalue"
        android:layout_alignBottom="@+id/rtankafvalue"
        android:layout_toEndOf="@+id/textView7"
        android:ems="10"
        android:hint="C"
        android:imeOptions="actionNext"
        android:inputType="numberDecimal" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/aftext"
        android:layout_toEndOf="@+id/ltankb4value"
        android:layout_toStartOf="@+id/rtankb4value"
        android:text="+"
        android:textSize="10sp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView2"
        android:layout_toEndOf="@+id/ctankb4value"
        android:text="="
        android:textSize="10sp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/aftext"
        android:layout_toEndOf="@+id/rtankb4value"
        android:text="+"
        android:textSize="10sp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/ltankafvalue"
        android:layout_toEndOf="@+id/ltankafvalue"
        android:text="+"
        android:textSize="10sp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView5"
        android:layout_alignTop="@+id/aftext"
        android:text="+"
        android:textSize="10sp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/afvalue"
        android:layout_toEndOf="@+id/ctankafvalue"
        android:text="="
        android:textSize="10sp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/ltankb4value"
        android:layout_alignStart="@+id/rtankb4value"
        android:text="+"
        android:textSize="10sp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button" />


</android.widget.RelativeLayout>

